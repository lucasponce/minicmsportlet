<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.5"
 xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
 <display-name>minicmsportlet</display-name>
 <servlet>
  <display-name>ImageCMS</display-name>
  <servlet-name>ImageCMS</servlet-name>
  <servlet-class>servlet.ImageCMS</servlet-class>
 </servlet>
 <servlet>
  <servlet-name>ContentCMS</servlet-name>
  <servlet-class>servlet.ContentCMS</servlet-class>
 </servlet>
 <servlet-mapping>
  <servlet-name>ImageCMS</servlet-name>
  <url-pattern>/image</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
  <servlet-name>ContentCMS</servlet-name>
  <url-pattern>/content</url-pattern>
 </servlet-mapping>
</web-app>
